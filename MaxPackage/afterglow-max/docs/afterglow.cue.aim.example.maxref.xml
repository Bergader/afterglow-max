    <metadata name="tag">DeepSymmetry</metadata>
    <metadata name="tag">DMX</metadata>
    <metadata name="tag">mxj</metadata>
    <metadata name="tag">Light Shows</metadata>
  </metadatalist>
	
  <!--SEEALSO-->
  <seealsolist>
    <seealso name="afterglow.max.Cue"/>
  </seealsolist>
</c74object>
